{
  "manifest_version": 3,
  "name": "my-extension",
  "version": "1.0.0",
  "description": "",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "web_accessible_resources": [
    {
      "resources": ["loading.json"],
      "matches": ["<all_urls>"]
    }
  ]
}
